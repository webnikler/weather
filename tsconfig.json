{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@api": [
        "./core/api"
      ],
      "@builders/*": [
        "./core/utils/builders/*"
      ],
      "@converters/*": [
        "./core/utils/converters/*"
      ],
      "@data/*": [
        "./core/utils/data/*"
      ],
      "@hooks/*": [
        "./core/utils/hooks/*"
      ],
      "@features/*": [
        "./features/*"
      ]
    }
  }
}