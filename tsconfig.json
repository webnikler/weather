{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@app/api": [
        "./shared/api"
      ],
      "@app/builders/*": [
        "./shared/utils/builders/*"
      ],
      "@app/converters/*": [
        "./shared/utils/converters/*"
      ],
      "@app/data/*": [
        "./shared/utils/data/*"
      ],
      "@app/hooks/*": [
        "./shared/utils/hooks/*"
      ],
      "@app/components": [
        "./shared/components"
      ],
      "@app/features/*": [
        "./features/*"
      ]
    }
  }
}